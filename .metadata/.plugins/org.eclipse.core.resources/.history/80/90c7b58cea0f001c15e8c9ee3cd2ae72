package part1SessionOne;

/*BUILD A RIGHT ANGLE TRIANGLE*/
public class Ex5 {
	public static void main(String[] args) {
		for(int i=0; i<5; i++) {
			for(int j =0; j<5; j++) {
				if() {
					System.out.println(" ");
				}else {
					System.out.print("*");
				}
			}
			System.out.print("\n");
		}
	}
}
