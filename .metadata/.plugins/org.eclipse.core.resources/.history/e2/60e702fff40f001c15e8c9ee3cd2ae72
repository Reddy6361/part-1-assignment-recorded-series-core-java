package part1SessionThree;

/*CHECK STRING IF IT STARTS/ENDS WITH ANY LETTER ?*/
public class Ex3 {
	public static void main(String[] args) {
		String str = "aabccde";
		System.out.println(str.startsWith("a"));
		System.out.println(str.endsWith("e"));
		System.out.println(str.startsWith("a"));
		System.out.println(str.endsWith("e"));
	}
}
