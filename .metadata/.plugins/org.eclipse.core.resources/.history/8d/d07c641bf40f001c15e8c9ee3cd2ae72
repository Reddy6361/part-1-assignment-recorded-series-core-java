package part1SessionThree;

/*STRING TO UPPERCASE & LOWERCASE*/
public class Ex1 {
	public static void main(String[] args) {
		String str = "pAnkaJ Sir ACAdemy";
		System.out.println(str.toLowerCase()); // we have built in method to convert into lower case.
		System.out.println(str.toUpperCase());// we have built in method to convert into upper case.
	}
}
