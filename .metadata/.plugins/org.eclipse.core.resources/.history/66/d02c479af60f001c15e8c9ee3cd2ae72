package part1SessionThree;

/*Assignment 1
 * WRITE A PROGRAM TO FIND HOW MANY a's and b's are there in (aabaaaabaa)?*/
public class Ex6 {
	public static void main(String[] args) {
		String str = "aabaaaabaa";
		int a=0;
		int b =0;
		for (int i = 0; i < str.length(); i++) {
			if(str.charAt(i)=='a') {
				a++;
			}
		}
	}
}
